package main.com.wiesfight.dto.enums;

public enum Items {
	KLAWIATURA("klawiatura", Bonuses.ATTACKPOWER, 1.2, 3),
	FLANELA("koszula", Bonuses.ATTACKPOWER, 1.2, 3),
	KAWA("kawa", Bonuses.ATTACKPOWER, 1.2, 3);
	
	private String filename;
	
	private Bonuses bonusType;
	
	private double bonus;
	
	private int duration;
	
	private Items(String filename, Bonuses bonusType, int bonus, int duration) {
		this.filename = filename;
		this.bonusType = bonusType;
		this.bonus = bonus;
		this.duration = duration;
	}
}
